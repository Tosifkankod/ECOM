{
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      },
      {
        "src": "build/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        {
            "src": "/api/v1/order/new", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/order/:id", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/orders/me", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/admin/orders", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/admin/order/:id", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/payment/process", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/stripeapikey", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/admin/products", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/products", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/admin/product/new", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/admin/product/:id", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/product/:id", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/review", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/reviews", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/register", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/login", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/password/forgot", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/password/reset/:token", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/logout", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/me", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/password/update", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/me/update", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/admin/users", 
            "dest": "backend/index.js"
        },
        {
            "src": "/api/v1/admin/user/:id", 
            "dest": "backend/index.js"
        }
    ],
  
    "rewrites": [{ "source": "/(.*)", "destination": "backend/server.js" }]
  }